<?xml version="1.0" encoding="UTF-8"?>
<:schema xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <xs:element name = "concesionario">
        <xs:complexType>
            <xs:sequence>
                <xs:element name = "coche" maxOccurs = "0">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name = "marca" type = "xs.string"></xs:element>
                            <xs:element name = "modelo" type = "xs.string"></xs:element>
                            <xs:choice>
                                <xs:element name = "cilindrada" type = "xs:decimal"></xs:element>
                                <xs:element name = "caballos" type = "xs:integer"></xs:element>
                            </xs:choice>
                            <xs:element name = "combustible" type = "xs.string"></xs:element>
                            <xs:element name = "precio" type = "xs.string"></xs:element>
                            <xs:element name = "año" type = "xs.integer"></xs:element>
                            <xs:element name = "color" type = "xs.string"></xs:element>
                            <xs:element name = "interesado" type = "xs.string" minOccurs = "0" maxOccurs = "unbounded"></xs:element>
                        </xs:sequence>
                        <xs:attribute name = "matricuñas" type = "xs.string" use = "required"></xs:attribute>
                    </xs:complexType>
                </xs:element>
            </xs:sequence>
            <xs:attribute name = "provincia" type = "xs:string"></xs:attribute>
        </xs:complexType>
    </xs:element>
</:schema>